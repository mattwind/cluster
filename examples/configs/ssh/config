# This is the default ssh 'hosts' file.
#
# It should live in ~/.ssh/config
#
#   - Comments begin with the '#' character
#   - Blank lines are ignored
#   - You can enter hostnames or ip addresses

Host node00
  Hostname 172.16.0.200
  User admin
  IdentityFile "/home/mwind/.ssh/cluster/id_rsa"

Host node01
  Hostname 172.16.0.201
  User admin
  IdentityFile "/home/mwind/.ssh/cluster/id_rsa"
